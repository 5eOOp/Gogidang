<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	 
<mapper namespace="com.spring.mapper.ReviewReplyMapper">
	
	<insert id="insert">
		insert into review_reply (comment_num,review_num,u_id,re_content)
		values (seq_review_reply.nextval,#{review_num} #{u_id},#{re_content},#{re_date})
	</insert>
	


</mapper>